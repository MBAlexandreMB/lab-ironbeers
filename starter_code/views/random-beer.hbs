{{#each beer}}
<div id="random-beer-container">
<figure>
  <img src="{{this.image_url}}"/>
</figure>
<div id="random-beer-text-container">
  <h1>{{name}}</h1>
  <h2>{{tagline}}</h2>
  <p>{{description}}</p>
  <h3>Food Pairing</h3>
  <ul>
    {{#each food_pairing}}
      <li>{{this}}</li>
    {{/each}}
  </ul> 
  <p><b>Brew Tips: </b>{{brewers_tips}}</p>
</div>
</div>
{{/each}}